{
  "name": "ipfs-gui",
  "version": "1.0.0",
  "description": "IPFS app",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "author": "F",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3"
  },
  "build": {
    "appId": "com.electron.ipfsapp",
    "productName": "ipfsApp",
    "files": [
      "css/**",
      "html/**",
      "js/**",
      "node_modules/**",
      "main.js",
      "preload.js",
      "data.json",
      "!.ipfs/**",
      "!getdata/**",
      "!lib/**",
      "!providers/**",
      "!putTMP/**",
      "!attr",
      "!kadrtt.properties",
      "!run.bat"
    ],
    "extraFiles": [
      {
        "from": ".ipfs/",
        "to": ".ipfs/"
      },
      {
        "from": "getdata/",
        "to": "getdata/"
      },
      {
        "from": "lib/",
        "to": "lib/"
      },
      {
        "from": "providers/",
        "to": "providers/"
      },
      {
        "from": "putTMP/",
        "to": "putTMP/"
      },
      {
        "from": "attr",
        "to": "attr"
      },
      {
        "from": "kadrtt.properties",
        "to": "kadrtt.properties"
      },{
        "from": "run.bat",
        "to": "run.bat"
      },
      {
        "from": "run.sh",
        "to": "run.sh"
      }
    ]
  }
}
